function ball_update()
	 --physics
  ball.dy+=gravity * 0.5
  ball.dx*=friction
  
  --check collision up and down
  if ball.dy>0 then
    ball.dy=limit_speed(ball.dy,ball.max_dy)

    if collide_map(ball,"down",0) then
      ball.dy=0
      ball.y-=((ball.y+ball.h+1)%8)-1
      score = 0
    end
  elseif ball.dy<0 then
    if collide_map(ball,"up",1) then
      ball.dy=0
    end
  end
  
  --check collision left and right
  if ball.dx<0 then

    ball.dx=limit_speed(ball.dx,ball.max_dx)
				
    if collide_map(ball,"left",1) 
    then
      bounce()
    end
  elseif ball.dx>0 then

    ball.dx=limit_speed(ball.dx,ball.max_dx)

    if collide_map(ball,"right",1) then
        bounce()
    end
  end
  
  ball.x+=ball.dx
  ball.y+=ball.dy
  
  --limit ball to map
  if ball.x<map_start then
    ball.x=map_start
  end
  if ball.x>map_end-ball.w then
    ball.x=map_end-ball.w
  end
end

function ball_animate()
    ball.sp+=ball.dx
    if ball.sp>20 then
      ball.sp=18
    end
    if ball.sp<18 then
    		ball.sp=20
  end
end

function hit_ball(dir, force)
		if last_bump > time()-0.1
		or last_hit > time()-0.1
		then return
		end
		last_hit = time()
		ball.dy = force * (rnd(0.4)+0.8)
		ball.dx = dir * (rnd(0.5) + 0.5)
		score+=1
end

function bounce()
		ball.dx *= -1
end
