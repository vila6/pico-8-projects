function player_update()
  --physics
  player.dy+=gravity
  player.dx*=player.friction

  --controls
  if btn(‚¨ÖÔ∏è) then
    player.dx-=player.acc
    player.running=true
    player.flp=true
  end
  if btn(‚û°Ô∏è) then
    player.dx+=player.acc
    player.running=true
    player.flp=false
  end

  --slide
  if player.running
  and not btn(‚¨ÖÔ∏è)
  and not btn(‚û°Ô∏è)
  and not player.falling
  and not player.jumping then
    player.running=false
    player.sliding=true
  end

  --jump
  if btnp(üÖæÔ∏è)
  and player.landed then
    player.dy-=player.boost
    player.landed=false
  end

		--dash start
		if btnp(‚ùé)
		and player.can_dash
		and last_bump < time() -0.2 then
				player.dashing=true
				player.can_dash=false
				player.dash_last=time()

		end
		
		--dash during
		if player.dashing
		and time() - player.dash_last
						< player.dash_time then
				player.dy = 0
				player.dx = player.dash_spd	
				if player.flp then
				  player.dx *= -1
				end
		else
				player.dashing=false		
		end
  --check collision up and down
  if player.dy>0 then
    player.falling=true
    player.landed=false
    player.jumping=false
    player.bumping=false

    player.dy=limit_speed(player.dy,player.max_dy)

    if collide_map(player,"down",0) then
      player.landed=true
      player.can_dash=true
      player.falling=false
      player.dy=0
      player.y-=((player.y+player.h+1)%8)-1
    end
  elseif player.dy<0 then
    if not player.bumping then
    player.jumping=true
    end
    if collide_map(player,"up",1) then
      player.dy=0
    end
  end

  --check collision left and right
  if player.dx<0 then

    player.dx=limit_speed(player.dx,player.max_dx)
				
    if collide_map(player,"left",1) 
    then
      if player.dashing then
      		bump()
      else
      player.dx=0
      end
    end
  elseif player.dx>0 then

    player.dx=limit_speed(player.dx,player.max_dx)

    if collide_map(player,"right",1) then
      if player.dashing then
      		bump()
      else
      		player.dx=0
      end
    end
  end

		--ball interaction
		if dist(player,ball) < 8
				then 
				if player.dashing then
						hit_ball(player.dx, -3)
						bump()
				elseif player.jumping
						and player.y > ball.y then
						hit_ball(rnd(4)*2-4, player.dy/2-1)
						player.dy = 0
				elseif player.y > ball.y then
						hit_ball(rnd(5)*2-5, -3)
				end
		end
  --stop sliding
  if player.sliding then
    if abs(player.dx)<.2
    or player.running then
      player.dx=0
      player.sliding=false
    end
  end

  player.x+=player.dx
  player.y+=player.dy

  --limit player to map
  if player.x<map_start then
    player.x=map_start
  end
  if player.x>map_end-player.w then
    player.x=map_end-player.w
  end
end

function bump()
		last_bump = time()
		player.dashing = false
		player.bumping = true
		player.jumping = false
 	player.dx *= -0.5
 	player.dy -= player.boost
  player.can_dash = true
end 

function player_animate()
  if player.dashing then
  		player.sp=10
  elseif player.bumping then
  		player.sp=5
  elseif player.jumping then
    player.sp=7
  elseif player.falling then
    player.sp=8
  elseif player.sliding then
    player.sp=9
  elseif player.running then
    if time()-player.anim>.1 then
      player.anim=time()
      player.sp+=1
      if player.sp>4 then
        player.sp=3
      end
    end
  else --player idle
    if time()-player.anim>.3 then
      player.anim=time()
      player.sp+=1
      if player.sp>2 then
        player.sp=1
      end
    end
  end
end

function limit_speed(num,maximum)
  return mid(-maximum,num,maximum)
end
