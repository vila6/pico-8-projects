--helpers
function lerp(a,b,t)
  return a+(b-a)*t
end

function are_closer_than(d, x1,y1, x2,y2)
  local dx = abs(x2-x1)
  local dy = abs(y2-y1)
  return dx < d 
     and dy < d 
     and (dx^2 + dy^2) < d^2
end

function formated_time()
	t = time()
	s = flr(t%60)
	m = flr(t/60)
	return tostr(m)..":"..tostr(s)
end
