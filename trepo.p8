--game loop
function _init()
 generatelevel()
end

input={x=0,y=0}
player={ax=64, ay=64}
aux={x0=0.0,y0=0.0,x2=0.0,y2=0.0,a1=0.0,a2=0.0}
l=true
gameover=false

function _update60()
		read_input()
		p.a1_a+=input.x
		cam_tick(p.x0, p.y0)

		if btnp(ðŸ…¾ï¸) and cangrab(p.x2,p.y2) then
				l = not l
				aux.x0 = p.x0
				aux.y0 = p.y0
				aux.x2 = p.x2
				aux.y2 = p.y2
				
				aux.a1 = p.a1
				aux.a2 = p.a2
				
				p.x0 = aux.x2
				p.y0 = aux.y2
				p.x2 = aux.x2
				p.y2 = aux.y2
				
				aux.a1 = p.a1
				aux.a2 = p.a2
				
				p.a1 = 0.5-(1.0-aux.a2)
				p.a2 = 1.0-(0.5-aux.a1)
	
				p.a2_v = 0
				p.a2_a = 0

		end
		
		p.tick()
end

function _draw()
		cls(12)
		drawlevel()
		print(formated_time(),cam.x,cam.y)
	 line(p.x1,p.y1,p.x0,p.y0,1)
	 line(p.x1,p.y1,p.x2,p.y2,1)
		circfill(p.x0,p.y0,2,l and 8 or 9)
	 circfill(p.x1,p.y1,5,0)
	 circfill(p.x1,p.y1,3,10)
	 circfill(p.x2,p.y2,3,l and 9 or 8)
		
		if gameover then
				print("game over",cam.x+64,cam.y+64)
		end
end

function cangrab(px, py)

		for i=1,#grabables do
				if are_closer_than(grabables[i][3],grabables[i][1],grabables[i][2],px,py) then
					return true
				end
		end
end
