--camera

cam={x=64,y=64}

function cam_tick(tx, ty)
  tx -= 64
  ty -= 64
  
  cam.x = lerp(cam.x, tx, 0.1)
  cam.y = lerp(cam.y, ty, 0.1)
  
  camera(cam.x,cam.y)
end
