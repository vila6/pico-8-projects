--level
numgrabeables=200
grabables={}
winpoint={0,0}

function generatelevel()
		x=0
		y=0
		r=6
		
		for i=0,numgrabeables do
				add(grabables,{x,y,r})
				x+=rnd(40)-15
				y+=rnd(20)-8
				r=rnd(12)+3
		end
		winpoint = {grabables[#grabables-1][1],grabables[#grabables-1][2]}
end

function drawlevel()
		for i=1,#grabables do
				circfill(grabables[i][1],grabables[i][2],grabables[i][3],6)
		end
		for i=1,#grabables do
				circfill(grabables[i][1],grabables[i][2],grabables[i][3]-1,7)
		end
		circfill(winpoint[1],winpoint[2],grabables[#grabables-1][3],10)
				
end
